using System;
using System.Security.Cryptography;
using System.Text;

namespace Lz3_4_1
{ 

class Program
{
    static void Main()
    {
        string data = "Hello, world!"; // Ваші дані для хешування

        Console.WriteLine("Data: " + data);
        Console.WriteLine();

        // Вибір різних алгоритмів хешування
        HashAlgorithm[] algorithms = new HashAlgorithm[]
        {
            MD5.Create(),
            SHA1.Create(),
            SHA256.Create(),
            SHA384.Create(),
            SHA512.Create()
        };

        foreach (HashAlgorithm algorithm in algorithms)
        {
            byte[] hash = algorithm.ComputeHash(Encoding.UTF8.GetBytes(data));
            Console.WriteLine($"{algorithm.GetType().Name}: {BitConverter.ToString(hash)} (Length: {hash.Length})");
        }
      }
   }
}
